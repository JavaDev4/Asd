
        SELECT pd.PROPERTY_DETAILS_UUID
        FROM PROPERTY_DETAILS pd
        INNER JOIN (
            SELECT LOAN_NUMBER, MIN(TO_NUMBER(SOURCE_ID_2)) AS min_source_id_2
            FROM PROPERTY_DETAILS
            WHERE LOAN_NUMBER = #{item.loanNumber}
            GROUP BY LOAN_NUMBER
        ) min_pd ON pd.LOAN_NUMBER = min_pd.LOAN_NUMBER AND pd.SOURCE_ID_2 = min_pd.min_source_id_2
        WHERE pd.LOAN_NUMBER = #{item.loanNumber}
        LIMIT 1
    )
